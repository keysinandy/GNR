{
  "filesCompile": [
    {
      "filename": "package.json",
      "outputPath": "package.json",
      "ejs": "ejs/package.ejs",
      "merge": true
    },
    {
      "filename": "src/app.js",
      "outputPath": "src/app.js",
      "ejs": "ejs/app.ejs",
      "merge": false
    }
  ],
  "dirRemove": [
  "_ejs_build_",
  "ejs"
  <% if (!fast) { %>
    ,
    "src/page",
    "src/routes",
    "src/store"
  <% } %>
  ]
}
